  <% if @jobs.present? %>
    <section class="job-position-block all-vacancies-style">
    <div class="we-hire-section-bg Ruby">
      <div class="container">
        <% if @jobs.present? %>
          <% @jobs.each do |job| %>
            <div class="row">
              <div class="job-position-content-left">
                <h3><a href="<%= job_path(job) %>"><%= job.title %></a></h3>
                <p>
                  <%= job.description %>
                </p>
              </div>
              <div class="job-position-content-right">
                <h4>Requirements Summary:</h4>
                <ul>
                  <% job.job_skills.each do |skill| %>
                    <li><%= skill.skill_required %></li>
                  <% end %>
                </ul>
                <a href="<%= job_path(job) %>">LEARN MORE &amp; APPLY</a></div>
            </div>
          <% end %>
        <% end %>
      </div>
    </div>
  </section>
  <% else %>
<!--    <div> No job available at now.</div>-->
  <% end %>
