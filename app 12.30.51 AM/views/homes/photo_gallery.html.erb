<% content_for :title do %>
  Syftet Photo Gallary Dhaka , Bangladesh | Syftet Limited
<% end %>
<% content_for :meta_title do %>
  Syftet Photo Gallary | Syftet
<% end %>

<div class="custom custom-software-dev">
  <header id="header" class="header">
    <div class="header-content">
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
            <div class="text-container">
              <h1>Welcome To Photo Gallary</h1>
            </div>
          </div>
        </div>
      </div>
    </div>
  </header>
  <section id="photo_gallery" class="section-md bg-gray-lighter text-center">
    <div class="">
      <h2 class="text-center">Our Festival Glory</h2>
      <%= render 'homes/shared/photo_gallery', photos: @photos %>
    </div>
  </section>
  <%= render 'homes/shared/start_project_form', contact: @contactinfo %>
</div>
